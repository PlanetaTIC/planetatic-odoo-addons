<data>

<record id="view_partner_fedicom_form" model="ir.ui.view">
    <field name="name">partner.fedicom.form.inherit</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
        <page name='sales_purchases' position="after">
            <page name="fedicom" string="Fedicom" >
                <group name="fedicom_credentials">
	                <field name="fedicom_user"/>
	                <field name="fedicom_password" widget="password"/>
	                <field name="fedicom_supplier_code" />
	                <field name="fedicom_order_type" />
	                <field name="fedicom_center" />
                </group>
            </page>
        </page>
    </field>
</record>

